{{ self.edition }};

package {{+ package }};

{% set sorted_imports = self.imports.as_sorted_vec() ~%}

{%~ for import in sorted_imports ~%}
import "{{ import }}";
{% endfor ~%}

{%~ if let Some(options) = self.render_options() +%}
{{~ options }}
{% endif +%}

{% for extension in extensions ~%}
extend {{+ extension.target +}} {
  {% for field in extension.fields ~%}
  {{+ field.render(self.package) }}
  {% endfor ~%}
}
{%~ endfor %}

{%~ for enum_ in enums ~%}
{{~ enum_ ~}}
{%~ endfor %}

{%~ for msg in messages ~%}
{{~ msg ~}}
{%~ endfor %}

{%~ for service in services ~%}
{{~ service ~}}
{%~ endfor %}
